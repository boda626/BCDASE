<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".pokemonSearch">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/pokemonID0"
                android:layout_width="50dp"
                android:layout_height="40dp"
                android:layout_weight="1"
                android:text="ID" />

            <TextView
                android:id="@+id/pokemonID"
                android:layout_width="50dp"
                android:layout_height="40dp"
                android:layout_weight="1" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/pokemonName0"
                android:layout_width="50dp"
                android:layout_height="40dp"
                android:layout_weight="1"
                android:text="Name" />

            <TextView
                android:id="@+id/pokemonName"
                android:layout_width="50dp"
                android:layout_height="40dp"
                android:layout_weight="1" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="41dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/baseExperience0"
                android:layout_width="50dp"
                android:layout_height="40dp"
                android:layout_weight="1"
                android:text="Base Experience" />

            <TextView
                android:id="@+id/baseExperience"
                android:layout_width="50dp"
                android:layout_height="40dp"
                android:layout_weight="1" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="41dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/height0"
                android:layout_width="50dp"
                android:layout_height="40dp"
                android:layout_weight="1"
                android:text="Height" />

            <TextView
                android:id="@+id/height"
                android:layout_width="50dp"
                android:layout_height="40dp"
                android:layout_weight="1" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="41dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/isDefault0"
                android:layout_width="50dp"
                android:layout_height="40dp"
                android:layout_weight="1"
                android:text="Is Default" />

            <TextView
                android:id="@+id/isDefault"
                android:layout_width="50dp"
                android:layout_height="40dp"
                android:layout_weight="1" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="44dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/order0"
                android:layout_width="50dp"
                android:layout_height="40dp"
                android:layout_weight="1"
                android:text="Order" />

            <TextView
                android:id="@+id/order"
                android:layout_width="50dp"
                android:layout_height="40dp"
                android:layout_weight="1" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="38dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/weight0"
                android:layout_width="50dp"
                android:layout_height="40dp"
                android:layout_weight="1"
                android:text="Weight" />

            <TextView
                android:id="@+id/weight"
                android:layout_width="50dp"
                android:layout_height="40dp"
                android:layout_weight="1" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="43dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/type0"
                android:layout_width="50dp"
                android:layout_height="40dp"
                android:layout_weight="1"
                android:text="Type" />

            <TextView
                android:id="@+id/type"
                android:layout_width="50dp"
                android:layout_height="40dp"
                android:layout_weight="1" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="41dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/type20"
                android:layout_width="50dp"
                android:layout_height="40dp"
                android:layout_weight="1"
                android:text="Type 2" />

            <TextView
                android:id="@+id/type2"
                android:layout_width="50dp"
                android:layout_height="40dp"
                android:layout_weight="1" />
        </LinearLayout>

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="285dp"
            android:layout_height="193dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="parent" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>